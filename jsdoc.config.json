{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },    
    "source": {
        "include": [
            "lib/Api.js", 
            "lib/Cache.js",
            "lib/Context.js",
            "lib/Database.js",
            "lib/Exception.js",
            "lib/Scrubber.js",
            "lib/Store.js",
            "lib/index.js",
            "test/Scrubber.js"
        ]
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "./docs/jsdoc/",
        "recurse": true,
        "tutorials": "./docs/tutorials"
    },
    "templates": {
        "default": {
            "outputSourceFiles": false,
            "includeDate": false
        },
        "cleverLinks": false,
        "monospaceLinks": false
    }
}